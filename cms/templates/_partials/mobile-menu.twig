{% set nodes = craft.navigation.nodes('primaryNavigation').all() %}

{# clean up & minimize what we push through the props as JSON #}
{% set mobilePages = [] %}
{% for item in nodes %}
    {% set mobilePages = mobilePages|merge([{
        id: item.id,
        url: item.url,
        title: item.title,
        level: item.level,
    }]) %}
{% endfor %}

<MobileMenu :pages="{{mobilePages|json_encode}}"></MobileMenu>